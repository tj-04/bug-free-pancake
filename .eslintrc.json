{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": [2, {
          "code": 1000,
          "tabWidth": 2,
          "ignoreUrls": true,
          "ignorePattern": "^goog\\.(module|require)"
        }]
      }
}
